{
    "name": "abirchler/laminas-rest-api",
    "description": "Laminas module to build REST APIs",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/abirchler/laminas-rest-api",
    "authors": [
        {
            "name": "abirchler",
            "role": "Author"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/abirchler/laminas-rest-api/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/abirchler/laminas-rest-api/issues",
        "source": "https://github.com/abirchler/laminas-rest-api"
    },
    "keywords": [
        "lamians",
        "laminas framework",
        "cors",
        "rest",
        "rest api"
    ],
    "require": {
        "firebase/php-jwt": "^6.0",
        "laminas/laminas-json": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "RestApi\\": "src/"
        }
    },
    "extra": {
        "laminas": {
            "module": "RestApi"
        }
    }
}
